utifulSoup as bs\n","import csv\n","import time, re\n","\n","# 재료 추출 함수\n","def extract_ingre(soup):\n","    ingre_list = []\n","    a_tags = soup.find_all('a', {'href': re.compile(r\"javascript:viewMaterial\\('\\d+'\\);\")})\n","    for a_tag in a_tags:\n","        img_tag = a_tag.find_next('img')\n","        if img_tag:\n","            text_between = img_tag.previous_sibling.strip()\n","            ingre_list.append(text_between)\n","    return ingre_list\n","\n","# 레시피 데이터 추출 함수\n","def extract_recipe_data(soup):\n","    try:\n","        menu_name = soup.find('div', class_='view2_summary st3').find('h3').get_text(strip=True)\n","        menu_img = soup.find('div', class_='centeredcrop').find('img')['src']\n","        difficulty = soup.find('span', class_='view2_summary_info3').get_text(strip=True)\n","        cook_time = soup.find('span', class_='view2_summary_info2').get_text(strip=True)\n","        ingre_list = extract_ingre(soup)\n","        recipe_list = [step.get_text(strip=True) for step in soup.find('div', class_='view_step').find_all('div', class_='view_step_cont')]\n","\n","        return {\"name\": menu_name, \"img\": menu_img, \"difficulty\": difficulty, \"cooktime\": cook_time, \"ingre\": ingre_list, \"recipe\": recipe_list, \"classification\": \"볶음\"}\n","    except:\n","        return None\n","\n","# URL 함수\n","def url_func(n, m):\n","    num_range = range(n, m)\n","    url = \"https://www.10000recipe.com/recipe/list.html?cat1=1&order=reco&page=\"\n","    url_list = []\n","    for num in num_range:\n","        print(f\"페이지 {num}/{m-1} 처리 중...\")\n","        req = urllib.request.Request(url + str(num))\n","        code = urllib.request.urlopen(req).read()\n","        soup = bs(code, \"html.parser\")\n","        try:\n","            res = soup.find_all(class_='common_sp_list_li')\n","            for li in res:\n","                for i in li.find_all('a', href=True):\n","                    url_tmp = i['href']\n","                    if url_tmp not in url_list:\n","                        url_list.append(url_tmp)\n","        except AttributeError:\n","            pass\n","        time.sleep(1)\n","    return url_list\n","\n","# CSV 저장 함수\n","def toCSV(recipe_list, filename):\n","    with open(filename, 'w', encoding='utf-8', newline='') as file:\n","        writer = csv.writer(file)\n","        writer.writerow([\"id\", \"name\", \"img\", \"difficulty\", \"cooktime\", \"ingredients\", \"recipe\", \"classification\"])\n","        for recipe in recipe_list:\n","            writer.writerow([\n","                recipe[\"id\"],\n","                recipe[\"name\"],\n","                recipe[\"img\"],\n","                recipe[\"difficulty\"],\n","                recipe[\"cooktime\"],\n","                ', '.join(recipe[\"ingre\"]),\n","                ', '.join(recipe[\"recipe\"]),\n","                recipe[\"classification\"]\n","            ])\n","    print(f\"CSV 파일 저장 완료: {filename}\")\n","\n","# 크롤링 실행\n","num_id = 0\n","food_dicts = []\n","url_lists = url_func(1, 21)  # 1페이지부터 크롤링\n","\n","for idx, url_str in enumerate(url_lists, 1):\n","    print(f\"레시피 {idx}/{len(url_lists)} 처리 중...\")\n","    url = \"https://www.10000recipe.com\" + url_str\n","    req = urllib.request.Request(url)\n","    code = urllib.request.urlopen(req).read()\n","    soup = bs(code, \"html.parser\")\n","\n","    recipe_data = extract_recipe_data(soup)\n","    if recipe_data:\n","        num_id += 1\n","        food_dict = {\"id\": num_id}\n","        food_dict.update(recipe_data)\n","        food_dicts.append(food_dict)\n","\n","# CSV 파일 저장\n","toCSV(food_dicts, '끓이기.csv')\n","print(\"크롤링 완료\")"]}]}