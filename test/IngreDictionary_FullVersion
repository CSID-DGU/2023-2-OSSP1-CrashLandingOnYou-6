n","        df = pd.read_excel(file_path)\n","        return df\n","    except FileNotFoundError:\n","        return pd.DataFrame()\n","\n","# 토큰 읽어오기\n","potato_tokens = read_tokens_from_file(potato_file_path)\n","seafood_tokens = read_tokens_from_file(seafood_file_path)\n","\n","# 각 토큰을 엑셀 파일에 추가하기\n","append_to_excel(excel_file_path, potato_tokens)\n","append_to_excel(excel_file_path, seafood_tokens)\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"x8zCovVGZyQd","executionInfo":{"status":"ok","timestamp":1700277814042,"user_tz":-540,"elapsed":686,"user":{"displayName":"곽시헌","userId":"08857774292332952664"}},"outputId":"6a9375cc-2276-471b-b06f-ad066bc11985"},"execution_count":6,"outputs":[{"output_type":"stream","name":"stderr","text":["<ipython-input-6-a325e787d6b1>:22: FutureWarning: The frame.append method is deprecated and will be removed from pandas in a future version. Use pandas.concat instead.\n","  existing_data = existing_data.append(pd.DataFrame(data, columns=['Ingredient']), ignore_index=True)\n"]}]}]}